.list{
  background: #ffffff;
}
.list-item{
  display: flex;
  flex-direction: row;
  padding:29rpx 0rpx 17rpx 0;
  padding-left:31rpx; 
}
.list-img{
  margin-right: 27rpx;
}
.list-img image{
  border:2px solid #e0e0e0;
  width: 156rpx;
  height: 156rpx;
}
.list-info{
  width: 100%;
}
.list-info .list-info-title{
  font-size: 26rpx;
  font-weight: bold;
}
.list-info-score{
  padding:13rpx 0;
  font-size: 22rpx;
  display: flex;
}
.list-info-score-img{
  width: 131rpx;
  height: 25rpx;
  overflow: hidden;
  background-image: url("data:image/png;base64,");
  background-size: auto 25rpx;
}
.list-info-score-img .score{
  width: 70%;
  background-image: url("data:image/png;base64,");
  background-size: auto 25rpx;
  height: 100%;
}
.list-info-score-text{
  margin-left: 20rpx;
}
.list-info-fl{
  width: 100%;
  font-size: 23rpx;
  color:#666666;
  display: flex;
  justify-content: space-between;
}
.list-info-fl text{
  margin-right: 21rpx;
  color: #666666;
}
.list-info-tag{
  padding-top: 15rpx;
}
.list-info-tag text{
  display: inline-block;
  text-align: center;
  letter-spacing: 1px;
  padding:0 10rpx;
  height:38rpx;
  border:1px solid #999;
  line-height: 38rpx;
  font-size: 19rpx;
  margin-right: 10rpx;
 
}